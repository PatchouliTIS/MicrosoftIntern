<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <config>
    <add key="globalPackagesFolder" value=".\Nugets" />
    <add key="repositoryPath" value=".\NugetPackages" />
  </config>
  <packageRestore>
    <add key="enabled" value="True" />
    <add key="automatic" value="True" />
  </packageRestore>
  <packageSources>
    <clear />
    <!-- Please refer to wiki (https://msasg.visualstudio.com/Bing_and_IPG/_wiki/wikis/Bing_and_IPG.wiki/57333/Add-New-Feeds-to-Nuget.Config) for adding new feeds -->
    <add key="xap_consumption" value="https://msasg.pkgs.visualstudio.com/_packaging/xap_consumption/nuget/v3/index.json" />
    <add key="Bing" value="https://msasg.pkgs.visualstudio.com/_packaging/Bing/nuget/v3/index.json" />
    <add key="xap_services" value="https://msasg.pkgs.visualstudio.com/_packaging/xap_services/nuget/v3/index.json" />
  </packageSources>
</configuration>